arr=list(map(int,input().split()))
target=int(input())
ind=0
def check(ind,arr,target):
    if(target==0):
        return 1
    if(target<0 or ind==len(arr)):
        return 0
    path1=check(ind+1,arr,target-arr[ind])
    path2=check(ind+1,arr,target)
    return path1+path2
def perfectSum(arr,target):
    ind=0
print(check(ind,arr,target))
print(perfectSum(arr,target))

output:
1 2 3
2
1
None
    
